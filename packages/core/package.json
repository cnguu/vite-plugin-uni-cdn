{
  "name": "@cnguu/vite-plugin-uni-cdn",
  "type": "module",
  "version": "2.1.1",
  "description": "Vite 插件，在 uni-app 中替换资源链接为 CDN 链接",
  "license": "MIT",
  "homepage": "https://github.com/cnguu/vite-plugin-uni-cdn",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cnguu/vite-plugin-uni-cdn.git"
  },
  "bugs": "https://github.com/cnguu/vite-plugin-uni-cdn/issues",
  "keywords": [
    "vite",
    "plugin",
    "uni",
    "uniapp",
    "cdn"
  ],
  "maintainers": [
    {
      "name": "cnguu",
      "email": "gu642779596@gmail.com",
      "url": "https://github.com/cnguu"
    }
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./virtual": {
      "types": "./virtual.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": ["dist", "virtual.d.ts"],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "unbuild",
    "debug": "cross-env NODE_ENV=development unbuild --sourcemap",
    "stub": "unbuild --stub"
  },
  "peerDependencies": {
    "ali-oss": ">=6.0.0",
    "vite": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "ali-oss": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/ali-oss": "catalog:",
    "ali-oss": "catalog:",
    "rollup": "catalog:",
    "vite": "catalog:"
  }
}
